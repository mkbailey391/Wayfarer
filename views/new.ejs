<div class="row text-center">
        <div class="col">
            <h1>Create a Planet:</h1>
            <form method="post" action="/planets">
                <div class="form-group">
                    <label for="name">Name:</label>
                    <input type="text" class="form-control" name="name" placeholder="Enter Planet Name">
                </div>
                <div class="form-group">
                    <label for="imageUrl">Image Url:</label>
                    <input type="text" class="form-control" name="imageUrl" placeholder="Enter Planet Name">
                </div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
    </div>